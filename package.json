{
  "name": "mparticle-sdk-javascript",
  "version": "2.0.13",
  "description": "mParticle core Javascript SDK",
  "repository": "https://github.com/mParticle/mparticle-sdk-javascript",
  "scripts": {
    "test": "node test/runner.js",
    "testKarma": "node test/test-karma.js",
    "testBuild": "watchify test/tests.js -v -o test/test-bundle.js --debug",
    "watch": "watchify src/main.js -v -o build/mParticle-dev.js --debug"
  },
  "pre-commit": [
    "lint"
  ],
  "browserify": {
      "transform": [
          "aliasify"
      ]
  },
  "aliasify": {
    "aliases": {
        ".": "./src",
        "test": "./test"
    }
  },
  "devDependencies": {
    "aliasify": "^2.1.0",
    "browser-sync": "^2.18.5",
    "browserify": "^14.3.0",
    "chai": "^4.1.1",
    "eslint": "^3.17.1",
    "karma": "^1.7.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-edge-launcher": "^0.4.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-ie-launcher": "^1.0.0",
    "karma-junit-reporter": "^1.2.0",
    "karma-mocha": "^1.3.0",
    "karma-safari-launcher": "^1.0.0",
    "mocha": "^2.5.3",
    "pre-commit": "^1.2.2",
    "shelljs": "^0.7.8",
    "should": "^7.1.0",
    "sinon-browser-only": "^1.12.1",
    "watchify": "^3.9.0"
  }
}
